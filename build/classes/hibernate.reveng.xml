	</table-filter>
	<table catalog="bankelmaghrib" name="tenue">
		<column name="intitule"></column>
	</table>
	<table catalog="bankelmaghrib" name="categorie">
		<column name="intitule"></column>
	</table>
	<table catalog="bankelmaghrib" name="tenue_article">
		<column name="id_article"></column>
