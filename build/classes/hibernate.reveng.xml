	</table-filter>
	<table-filter match-name="mesure" match-catalog="bankelmaghrib"></table-filter>
	<table-filter match-name="poste" match-catalog="bankelmaghrib"></table-filter>
	<table-filter match-name="tenue" match-catalog="bankelmaghrib"></table-filter>
	<table-filter match-name="tenue_article"
