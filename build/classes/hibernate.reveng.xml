	</table>
	<table catalog="bankelmaghrib" name="poste">
		<column name="intitule"></column>
		<column name="tenue"></column>
	</table>
	<table catalog="bankelmaghrib" name="agent">
		<column name="id"></column>
		<column name="nom"></column>
		<column name="prenom"></column>
		<column name="poste"></column>
		<column name="mesure"></column>
		<column name="entite"></column>
		<column name="sexe"></column>
	</table>
</hibernate-reverse-engineering>